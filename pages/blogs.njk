  <div class="category-container">
    
        <div class="category-top-section">
            {%-for post in collections.top-%}
                <a href="{{post.url}}">
                    <div class="left-side">
                        
                            <img  src="{{post.data.image}}" alt="">
                            <h3>{{post.data.category}}</h3>
                            <h2>{{post.data.title}}</h2>
                            
                        
                    
                    </div>
                </a>
            {%endfor%}
            <div class="right-side">
                {%-for post in collections.featured-%}
                    <div class="right-side-row">
                        
                            <a href="{{post.url}}">
                                <div class="left-post">
                                    <img  src="{{post.data.image}}" alt="">
                                    <h3>{{post.data.category}}</h3>
                                    <h2>{{post.data.title}}</h2>
                                        
                                </div>
                            </a>
                        
                        
                            
                    </div>
                {%endfor%}
                
                
                
            </div>
        </div>
        
    <div class="blogs-container">
        {%- for post in collections.post -%}
            <a href="{{ post.url }}">
                <div class="blog-post">
                    <img src="{{ post.data.image }}" alt="">
                    <h3>{{ post.data.category }}</h3>
                    <h2>{{ post.data.title }}</h2>
                </div>
            </a>
        {%- endfor %}
    </div>





